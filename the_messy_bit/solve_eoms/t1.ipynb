     "output_type": "stream",
     "text": [
      "Solver: GaussNewton, Error: 1.5\n",
      "Solver: GaussNewton, Error: 1.1102230246251564e-16\n"
     ]
    },
    {
     "data": {
      "text/plain": "Array([-1. , -0.5], dtype=float64)"
     },
     "execution_count": 19,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "(jaxopt.GaussNewton(\n",
    "    residual_fun=fn,\n",
