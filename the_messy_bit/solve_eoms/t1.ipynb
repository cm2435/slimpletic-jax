    "import jaxopt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "outputs": [],
   "source": [
    "import jax.numpy as jnp\n",
    "\n",
    "def fn(x):\n",
    "    return jnp.array([\n",
    "        x[0] - 2 * x[1],\n",
    "        x[0]**2 - 2 * x[0] - 3,\n",
    "    ])"
   ],
   "metadata": {
    "collapsed": false,
    "ExecuteTime": {
     "end_time": "2023-11-27T15:02:21.234665Z",
     "start_time": "2023-11-27T15:02:21.229681Z"
    }
   },
   "id": "bc750b422072f831"
  },
  {
